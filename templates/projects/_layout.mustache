{{> header }}
{{> heading }}
<div class="about_content">
 <div class="proj_image">
    {{#images}}
       <img src="{{ image }}" />
    {{/images}}
 </div>
 <div class="proj_side" />
    <div class="proj_name">
        <h3> {{ name }} </h3>
    </div>
    <ul class="nav_list">
        <li><a href="{{prev}}">PREVIOUS</a></li>
        <li>|</li>
        <li><a href="{{next}}">NEXT</a></li>
        <li>|</li>
        <li><a href="../index.html">BACK TO GALLERY</a></li>
    </ul>
    <div class="description" />
        <h4> DESCRIPTION </h4>
        {{#description}}
            <p> {{ paragraph }} </p>
        {{/description}}
    </div>
    <div class="components">
        <h4> PROJECT COMPONENTS </h4>
        <ul>
        {{#proj_components}}
            <li>{{name}}</li>
        {{/proj_components}}
        </ul>
    </div>
    <div class="proj_navigation">
        <h4>OTHER PROJECTS</h4>
        <ul class="nav_list">
            <li><a href="{{prev}}">PREVIOUS</a></li>
            <li>|</li>
            <li><a href="{{next}}">NEXT</a></li>
            <li>|</li>
            <li><a href="../index.html">BACK TO GALLERY</a></li>
        </ul>
        <ul class="other_projects">
            {{#other_projects}}
                <li class="{{current}}"><a href="{{link}}">{{name}}</a></li>
            {{/other_projects}}
        </ul>
    </div>
 </div>
</div>
{{> footer }}
